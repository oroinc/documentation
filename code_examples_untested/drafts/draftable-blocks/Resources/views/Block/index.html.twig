{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set pageTitle = 'acme.cms.block.entity_plural_label'|trans %}
{% set gridName = 'acme-cms-block-grid' %}

{% block navButtons %}
    {% import 'OroUIBundle::macros.html.twig' as UI %}

    {% if is_granted('acme_cms_block_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('acme_cms_block_create'),
                'entity_label': 'acme.cms.block.entity_label'|trans
            }) }}
        </div>
    {% endif %}
{% endblock %}
